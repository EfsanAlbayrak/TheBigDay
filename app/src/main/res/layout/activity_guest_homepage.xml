<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GuestList"
    android:orientation="vertical"
    android:background="@drawable/w">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My Invited Weddings"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:textSize="30dp"
        android:textStyle="bold"
        android:fontFamily="@font/mavenpro_variablefont_wght"></TextView>

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:background="@color/white"
        android:id="@+id/invitedWeds"
        android:textSize="30dp"
        android:textStyle="bold"
        android:backgroundTint="@color/transp"
        android:fontFamily="@font/mavenpro_variablefont_wght"
        ></ListView>

</LinearLayout>